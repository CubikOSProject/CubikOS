global GetEDXFeature
GetEDXFeature:
    mov eax, 1
    cpuid
    mov eax, edx
    ret

global GetECXFeature
GetECXFeature:
    mov eax, 1
    cpuid
    mov eax, ecx
    ret

global ISRWrapper
align 4
ISRWrapper:
    pushad
    cld
    call 

global LoadPageMaps
LoadPageMaps:
    mov cr3, rdi
    ret